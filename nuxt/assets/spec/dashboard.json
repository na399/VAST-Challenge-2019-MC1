                },
                {
                  "test": "ShowCategory == 'All' & datum.cat == 'buildings'",
                  "scale": "xPosNum",
                  "field": "loc",
                  "offset": 20
                },
                {
                  "test": "ShowCategory == 'All' & datum.cat == 'roads_and_bridges'",
                  "scale": "xPosNum",
                  "field": "loc",
                  "offset": 35
                }
              ],
              "yc": [
                {
                  "test": "ShowCategory == 'Single'",
                  "scale": "yPosNum",
                  "field": "loc",
                  "offset": 25
                },
                {
                  "test": "ShowCategory == 'All' & datum.cat == 'shake_intensity'",
                  "scale": "yPosNum",
